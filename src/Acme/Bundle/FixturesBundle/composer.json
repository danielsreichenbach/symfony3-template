{
    "name": "danielsreichenbach/acme-fixture-bundle",
    "type": "symfony-bundle",
    "description": "Fixtures bundle for Acme.",
    "keywords": ["fixture"],
    "license": "MIT",
    "homepage": "http://marenkay.com/",
    "authors": [
        {
            "name": "Daniel S. Reichenbach",
            "email": "daniel@kogitoapp.com",
            "homepage": "http://marenkay.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/danielsreichenbach/symfony3-template/issues",
